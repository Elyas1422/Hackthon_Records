<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="addPane" stylesheets="@../values/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.modifyRankViewController">
   <children>
      <VBox fx:id="addpane" prefHeight="585.0" prefWidth="800.0" stylesheets="@../values/style.css">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="800.0">
               <children>
                  <Button fx:id="backbutton" mnemonicParsing="false" styleClass="roundedbutton" stylesheets="@../values/style.css" text="&lt;-" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                  </Button>
                  <Label fx:id="ModifyTitle" styleClass="boldLabel" stylesheets="@../values/style.css" text="Modify Rank for Student x" textFill="WHITE">
                     <font>
                        <Font name="â˜žAbdoMaster-Medium" size="36.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label styleClass="boldLabel" stylesheets="@../values/style.css" text="Rank:" textFill="WHITE">
                     <padding>
                        <Insets left="50.0" top="20.0" />
                     </padding>
                     <font>
                        <Font size="22.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="rankComboBox" prefHeight="25.0" prefWidth="220.0" />
               </children>
            </HBox>
            <Label styleClass="boldLabel" stylesheets="@../values/style.css" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets left="50.0" top="20.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="finishButton" mnemonicParsing="false" styleClass="roundedbutton" stylesheets="@../values/style.css" text="Save The Rank" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="30.0" left="600.0" top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
